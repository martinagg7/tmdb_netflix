{
  "builds": [
      {
          "src": "netflixuax/wsgi.py",
          "use": "@vercel/python",
          "config": {
              "buildCommand": "python3 manage.py migrate"
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "netflixuax/wsgi.py"
      }
  ]
}